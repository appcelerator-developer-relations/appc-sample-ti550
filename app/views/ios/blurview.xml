<Alloy>
	<Window title="BlurView">
		<View class="vbox">
			<TabbedBar onClick="doBlur">
				<Labels>
					<Label>Extra Light</Label>
					<Label>Light</Label>
					<Label>Dark</Label>
					<Label>Regular</Label>
					<Label>Prominent</Label>
				</Labels>
			</TabbedBar>
			<Label class="help">Click an effect to apply it to the image view.</Label>
			<ImageView class="banner">
				<BlurView id="blurView" />
			</ImageView>
		</View>
	</Window>
</Alloy>
